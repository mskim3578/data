def get_evaluation():
    model.evaluate(x_testS,y_testS)
    #평가하기
    #혼동행렬 출력하기
    results = model.predict(x_testS)
    from sklearn.metrics import confusion_matrix
    cm=confusion_matrix(np.argmax(y_testS,axis=-1),\
                        np.argmax(results,axis=-1))
    cm    
    #혼동행렬을 heatmap으로 출력하기
    import seaborn as sns
    plt.figure(figsize = (7, 7))
    sns.heatmap(cm, annot = True, fmt = 'd',cmap = 'Blues')
    plt.xlabel('예측 label', fontsize = 15)
    plt.ylabel('이미지 label', fontsize = 15)
    # plt.xticks(np.arange(0, 2, 0.5), labels=[' ', '불량', ' ', '우량'])
    # plt.yticks(np.arange(0, 2, 0.5), labels=[' ', '불량', ' ', '우량'])
    plt.show()
