
df = pd.read_csv("data/weather_test.csv")
df.info()
df.timestamp
df['dt'] = pd.to_datetime(df['timestamp'], format='%Y-%m-%d %H:%M:%S')
# 시간 분류 
df['hour'] = df['dt'].dt.hour
# 분 분류 
df['minute'] = df['dt'].dt.minute
df['minute'].unique()

df['hour'].value_counts()
ghour=df.groupby('hour')
x1=df['hour'].unique().tolist()
y1=ghour['air_temperature'].mean().tolist()
y2=ghour['dew_temperature'].mean().tolist()
